local char = plr.Character
char.HumanoidRootPart.CFrame = CFrame.new(-487.705688, 269.455963, 5.49729156, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end
})
local antTab = Window:MakeTab({
	Name = "Anti-Afk",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = antTab:AddSection({
	Name = "Anti-Afk"
})
antTab:AddButton({
	Name = "Ativar antiafk",
	Callback = function()
      local VirtualUser = game:GetService('VirtualUser')
game:GetService('Players').LocalPlayer.Idled:connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)
      OrionLib:MakeNotification({
	Name = "AntiAfk",
	Content = "AntiAfk ativado!",
	Image = "rbxassetid://4483345998",
	Time = 5
})
  	end    
})
