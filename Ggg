local Player = game.Players.localPlayer

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Nogrus Hub | ADMIN RNG", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local NogrusTab = Window:MakeTab({
	Name = "auto-Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = NogrusTab:AddSection({
	Name = "Coletar Lucky potin"
})
NogrusTab:AddButton({
	Name = "Coletar luck potion",
	Callback = function()
      		local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
 
local function collectPotion(potion)
    hrp.CFrame = potion.Prim.CFrame
    wait(1)
    fireproximityprompt(potion.Prim.ProximityPrompt)
end
 
while true do
    local foundPotion = false
 
    for _, potion in pairs(workspace.Misc:GetChildren()) do
        if potion.Name == "Luck Potion" then
            collectPotion(potion)
            foundPotion = true
            wait(2)
        end
    end
 
    if not foundPotion then
        wait(1)
    end
 
    wait(5)
			end
  	end    
})



local Section = NogrusTab:AddSection({
	Name = "Auto-Farm"
})
NogrusTab:AddButton({
	Name = "Auto-Spin",
	Callback = function()
      		print("Auto spin ativado!")
		  while wait(0.0001) do
local args = {
    [1] = false,
    [2] = false
}

game:GetService("ReplicatedStorage").Events.Spin:InvokeServer(unpack(args))
end
	end    
})
local TeleTab = Window:MakeTab({
	Name = "Teleport",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = TeleTab:AddSection({
	Name = "Obby fácil"
})
TeleTab:AddButton({
	Name = "Ir para o obby fácil",
	Callback = function()
      		local plr = game.Players.LocalPlayer
local char = plr.Character
char.HumanoidRootPart.CFrame = CFrame.new(-597.601, 274.456, 21.1674)
  	end    
})
TeleTab:AddButton({
	Name = "Terminar obby Fácil",
	Callback = function()
      		local plr = game.Players.LocalPlayer
local char = plr.Character
char.HumanoidRootPart.CFrame = CFrame.new(-1305.84998, 17.2887249, 2524.10791, 1, 0, 0, 0, 1, 0, 0, 0, 1)
  	end    
})
local Section = TeleTab:AddSection({
	Name = "Obby difícil"
})
TeleTab:AddButton({
	Name = "Ir para o obby difícil",
	Callback = function()
      local plr = game.Players.LocalPlayer
local char = plr.Character
char.HumanoidRootPart.CFrame = CFrame.new(-616.600708, 274.455963, 21.0327816, -0.0070681572, 0, -0.999975145, 0, 1, 0, 0.999975145, 0, -0.0070681572)
    end
})
TeleTab:AddButton({
	Name = "Terminar obby difícil",
	Callback = function()
      local plr = game.Players.LocalPlayer
local char = plr.Character
char.HumanoidRootPart.CFrame = CFrame.new(-1615.72827, -1.74074268, 2455.42383, 1, 0, 0, 0, 1, 0, 0, 0, 1)
    end
})
local Section = TeleTab:AddSection({
	Name = "Spawn"
})
