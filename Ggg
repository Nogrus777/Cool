local Player = game.Players.localPlayer

local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/thanhdat4461/OrionMoblie/main/source')))()
local Window = OrionLib:MakeWindow({Name = "ADMIN RNG", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})
local NogrusTab = Window:MakeTab({
	Name = "auto-spin",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})
local Section = NogrusTab:AddSection({
	Name = "auto luck potion"
})
NogrusTab:AddButton({
	Name = "Auto coletar potion luck",
	Callback = function()
      		local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local hrp = character:WaitForChild("HumanoidRootPart")
 
local function collectPotion(potion)
    hrp.CFrame = potion.Prim.CFrame
    wait(1)
    fireproximityprompt(potion.Prim.ProximityPrompt)
end
 
while true do
    local foundPotion = false
 
    for _, potion in pairs(workspace.Misc:GetChildren()) do
        if potion.Name == "Luck Potion" then
            collectPotion(potion)
            foundPotion = true
            wait(2)
        end
    end
 
    if not foundPotion then
        wait(1)
    end
 
    wait(5)
			end
  	end    
})



local Section = NogrusTab:AddSection({
	Name = "Auto-Farm"
})
NogrusTab:AddButton({
	Name = "Auto-Spin",
	Callback = function()
      		print("Auto spin ativado!")
		  while wait(0.0001) do
local args = {
    [1] = false,
    [2] = false
}

game:GetService("ReplicatedStorage").Events.Spin:InvokeServer(unpack(args))
end
	end    
})

